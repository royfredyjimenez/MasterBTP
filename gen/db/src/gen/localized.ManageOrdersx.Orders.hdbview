VIEW localized_ManageOrdersx_Orders AS SELECT
  Orders_0.ClientEmail,
  Orders_0.FirstName,
  Orders_0.LastName,
  Orders_0.CreatedOn,
  Orders_0.Reviewed,
  Orders_0.Approved,
  Orders_0.Country_code,
  Orders_0.Status
FROM localized_com_training_Orders AS Orders_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_ManageOrdersx_Countries AS Country ON (Country.code = Country_code)
)