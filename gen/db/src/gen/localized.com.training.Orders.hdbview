VIEW localized_com_training_Orders AS SELECT
  L.ClientEmail,
  L.FirstName,
  L.LastName,
  L.CreatedOn,
  L.Reviewed,
  L.Approved,
  L.Country_code,
  L.Status
FROM com_training_Orders AS L
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_sap_common_Countries AS Country ON (Country.code = Country_code)
)