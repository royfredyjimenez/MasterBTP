VIEW CatProd_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Address_Street,
  Suppliers_0.Address_City,
  Suppliers_0.Address_State,
  Suppliers_0.Address_PostalCode,
  Suppliers_0.Address_Country,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM product_catalog_sales_Suppliers AS Suppliers_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN CatProd_Products AS ToProduct ON (ToProduct.Supplier_ID = ID)
)