VIEW ManageOrdersx_Countries AS SELECT
  Countries_0.name,
  Countries_0.descr,
  Countries_0.code
FROM sap_common_Countries AS Countries_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN ManageOrdersx_Countries_texts AS texts ON (texts.code = code),
  MANY TO ONE JOIN ManageOrdersx_Countries_texts AS localized ON (localized.code = code AND localized.locale = SESSION_CONTEXT('LOCALE'))
)