COLUMN TABLE com_training_Orders (
  ClientEmail NVARCHAR(65) NOT NULL,
  FirstName NVARCHAR(30),
  LastName NVARCHAR(30),
  CreatedOn DATE,
  Reviewed BOOLEAN,
  Approved BOOLEAN,
  Country_code NVARCHAR(3),
  Status NVARCHAR(1),
  PRIMARY KEY(ClientEmail)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN sap_common_Countries AS Country ON (Country.code = Country_code)
)