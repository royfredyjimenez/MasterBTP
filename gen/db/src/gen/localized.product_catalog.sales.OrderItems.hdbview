VIEW localized_product_catalog_sales_OrderItems AS SELECT
  L.ID,
  L.Order_ID,
  L.Product_ID,
  L.Quantity
FROM product_catalog_sales_OrderItems AS L
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_product_catalog_sales_Orders AS "ORDER" ON ("ORDER".ID = Order_ID),
  MANY TO ONE JOIN localized_product_catalog_materials_Products AS "PRODUCT" ON ("PRODUCT".ID = Product_ID)
)