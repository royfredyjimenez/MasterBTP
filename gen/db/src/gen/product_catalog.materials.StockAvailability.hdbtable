COLUMN TABLE product_catalog_materials_StockAvailability (
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN product_catalog_materials_Products AS "PRODUCT" ON ("PRODUCT".ID = Product_ID),
  MANY TO MANY JOIN product_catalog_materials_StockAvailability_texts AS texts ON (texts.ID = ID),
  MANY TO ONE JOIN product_catalog_materials_StockAvailability_texts AS localized ON (localized.ID = ID AND localized.locale = SESSION_CONTEXT('LOCALE'))
)