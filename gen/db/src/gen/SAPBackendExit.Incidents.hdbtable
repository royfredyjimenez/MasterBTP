COLUMN TABLE SAPBackendExit_Incidents (
  IncidenceId NVARCHAR(4) NOT NULL,
  SapId NVARCHAR(40) NOT NULL,
  EmployeeId NVARCHAR(4) NOT NULL,
  CreationDate TIMESTAMP,
  CreationDateX BOOLEAN,
  Type NVARCHAR(2),
  TypeX BOOLEAN,
  Reason NVARCHAR(200),
  ReasonX BOOLEAN,
  PRIMARY KEY(IncidenceId, SapId, EmployeeId)
)