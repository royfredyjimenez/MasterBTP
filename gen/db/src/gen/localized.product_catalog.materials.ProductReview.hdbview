VIEW localized_product_catalog_materials_ProductReview AS SELECT
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.ID,
  L.Name,
  L.Rating,
  L.Comment,
  L.Product_ID
FROM product_catalog_materials_ProductReview AS L
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_product_catalog_materials_Products AS "PRODUCT" ON ("PRODUCT".ID = Product_ID)
)