CREATE VIEW localized_de_product_catalog_reports_Products AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (localized_de_product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

CREATE VIEW localized_fr_product_catalog_reports_Products AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (localized_fr_product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

