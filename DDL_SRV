FROM localized_de_product_catalog_materials_Products AS Products_0;

CREATE VIEW localized_fr_product_catalog_materials_SelProducts2 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM localized_fr_product_catalog_materials_Products AS Products_0;

CREATE VIEW localized_de_product_catalog_materials_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_de_product_catalog_materials_Products AS Products_0 LEFT JOIN localized_de_product_catalog_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW localized_fr_product_catalog_materials_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_fr_product_catalog_materials_Products AS Products_0 LEFT JOIN localized_fr_product_catalog_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

