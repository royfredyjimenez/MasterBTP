
CREATE TABLE product_catalog_materials_Products (
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Description NVARCHAR(5000),
  ImageUrl NVARCHAR(5000),
  ReleaseDate TIMESTAMP_TEXT DEFAULT CURRENT_TIMESTAMP,
  CreationDate DATE_TEXT DEFAULT CURRENT_DATE,
  DiscontinuedDate TIMESTAMP_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasure_ID NVARCHAR(2),
  Currency_ID NVARCHAR(3),
  DimensionUnits_ID NVARCHAR(2),
  Category_ID NVARCHAR(1),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_Categories (
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_StockAvailability (
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_UnitOfMeasures (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_DimensionUnits (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_Currencies (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_ProductReview (
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000),
  Rating INTEGER,
  Comment NVARCHAR(5000),
  Product_ID NVARCHAR(36),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_sales_Orders (
  ID NVARCHAR(36) NOT NULL,
  Date DATE_TEXT,
  Customer NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_sales_OrderItems (
  ID NVARCHAR(36) NOT NULL,
  Order_ID NVARCHAR(36),
  Product_ID NVARCHAR(36),
  Quantity INTEGER,
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_sales_Suppliers (
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Address_Street NVARCHAR(5000),
  Address_City NVARCHAR(5000),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  Email NVARCHAR(5000) DEFAULT 'correo@logaligroup.com',
  Phone NVARCHAR(5000),
  Fax NVARCHAR(5000),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_sales_SalesData (
  createdAt TIMESTAMP_TEXT,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP_TEXT,
  modifiedBy NVARCHAR(255),
  ID NVARCHAR(36) NOT NULL,
  DeliveryDate TIMESTAMP_TEXT,
  Revenue DECIMAL(16, 2),
  Product_ID NVARCHAR(36),
  Currency_ID NVARCHAR(3),
  DeliveryMonth_ID NVARCHAR(2),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_sales_Months (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE product_catalog_materials_Products_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_materials_Categories_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_materials_StockAvailability_texts (
  locale NVARCHAR(14) NOT NULL,
  ID INTEGER NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_materials_UnitOfMeasures_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_materials_DimensionUnits_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_materials_Currencies_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(5000),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_sales_Months_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(5000),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(locale, ID)
);

CREATE TABLE product_catalog_sales_Suppliers_texts (
  locale NVARCHAR(14) NOT NULL,
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(5000) NOT NULL,
  PRIMARY KEY(locale, ID)
);

CREATE VIEW CatProd_Products AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Category_1.Name AS Category,
  Products_0.UnitOfMeasure_ID AS ToUnitOfMeasure_ID,
  Products_0.Currency_ID AS ToCurrency_ID,
  Products_0.Category_ID AS FoCategory_ID,
  Products_0.DimensionUnits_ID AS ToDimesionUnit_ID
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_materials_Categories AS Category_1 ON Products_0.Category_ID = Category_1.ID);

CREATE VIEW CatProd_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Address_Street,
  Suppliers_0.Address_City,
  Suppliers_0.Address_State,
  Suppliers_0.Address_PostalCode,
  Suppliers_0.Address_Country,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM product_catalog_sales_Suppliers AS Suppliers_0;

CREATE VIEW CatProd_Reviews AS SELECT
  ProductReview_0.ID,
  ProductReview_0.Name,
  ProductReview_0.Rating,
  ProductReview_0.Comment,
  ProductReview_0.Product_ID
FROM product_catalog_materials_ProductReview AS ProductReview_0;

CREATE VIEW CatProd_StockAvailability AS SELECT
  StockAvailability_0.ID,
  StockAvailability_0.Description,
  StockAvailability_0.Product_ID AS ToProduct_ID
FROM product_catalog_materials_StockAvailability AS StockAvailability_0;

CREATE VIEW CatProd_VH_Categories AS SELECT
  Categories_0.ID AS Code,
  Categories_0.Name AS Text
FROM product_catalog_materials_Categories AS Categories_0;

CREATE VIEW CatProd_VH_UnitOfMeasure AS SELECT
  UnitOfMeasures_0.ID AS Code,
  UnitOfMeasures_0.Description AS Text
FROM product_catalog_materials_UnitOfMeasures AS UnitOfMeasures_0;

CREATE VIEW MyService_SuppliersProduct AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID,
  Supplier_1.Address_Street,
  Supplier_1.Address_City,
  Supplier_1.Address_State,
  Supplier_1.Address_PostalCode,
  Supplier_1.Address_Country
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID)
WHERE (Supplier_1.Address_PostalCode = 98074) AND (Products_0.Name = 'Bread');

CREATE VIEW MyService_SupplierToSales AS SELECT
  Supplier_1.Email,
  Category_2.Name,
  Currency_4.ID,
  Currency_4.Description
FROM ((((product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID) LEFT JOIN product_catalog_materials_Categories AS Category_2 ON Products_0.Category_ID = Category_2.ID) LEFT JOIN product_catalog_sales_SalesData AS SalesData_3 ON (SalesData_3.Product_ID = Products_0.ID)) LEFT JOIN product_catalog_materials_Currencies AS Currency_4 ON SalesData_3.Currency_ID = Currency_4.ID);

CREATE VIEW MyService_EntityInfix AS SELECT
  Supplier_1.Phone
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_sales_Suppliers AS Supplier_1 ON (Products_0.Supplier_ID = Supplier_1.ID) AND (Supplier_1.Name = 'Exotic Liquids'))
WHERE Products_0.Name = 'Bread';

CREATE VIEW MyService_EntityJoin AS SELECT
  Supp_1.Phone
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_sales_Suppliers AS Supp_1 ON (Supp_1.ID = Products_0.Supplier_ID) AND (Supp_1.Name = 'Exotic Liquids'))
WHERE Products_0.Name = 'Bread';

CREATE VIEW product_catalog_materials_SelProducts AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID
FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_materials_SelProducts1 AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID
FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_materials_SelProducts2 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_materials_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_materials_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW product_catalog_materials_ProjProducts AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID
FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_materials_ProjProducts2 AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
