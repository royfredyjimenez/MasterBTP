FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_materials_ProjProducts3 AS SELECT
  Products_0.ReleaseDate,
  Products_0.Name
FROM product_catalog_materials_Products AS Products_0;

CREATE VIEW product_catalog_reports_AverageRating AS SELECT
  ProductReview_0.Product_ID AS ProductId,
  avg(ProductReview_0.Rating) AS AverageRating
FROM product_catalog_materials_ProductReview AS ProductReview_0
GROUP BY ProductReview_0.Product_ID;

CREATE VIEW CatProd_Products_texts AS SELECT
  texts_0.locale,
  texts_0.ID,
  texts_0.Name,
  texts_0.Description
FROM product_catalog_materials_Products_texts AS texts_0;

CREATE VIEW MyService_SuppliersProduct_texts AS SELECT
  texts_0.locale,
  texts_0.ID,
  texts_0.Name,
  texts_0.Description
FROM product_catalog_materials_Products_texts AS texts_0;

CREATE VIEW localized_product_catalog_materials_Products AS SELECT
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.ImageUrl,
  L_0.ReleaseDate,
  L_0.CreationDate,
  L_0.DiscontinuedDate,
  L_0.Price,
  L_0.Height,
  L_0.Width,
  L_0.Depth,
  L_0.Quantity,
  L_0.Supplier_ID,
  L_0.UnitOfMeasure_ID,
  L_0.Currency_ID,
  L_0.DimensionUnits_ID,
  L_0.Category_ID
FROM (product_catalog_materials_Products AS L_0 LEFT JOIN product_catalog_materials_Products_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_Categories AS SELECT
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name
FROM (product_catalog_materials_Categories AS L_0 LEFT JOIN product_catalog_materials_Categories_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_StockAvailability AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  L_0.Product_ID
FROM (product_catalog_materials_StockAvailability AS L_0 LEFT JOIN product_catalog_materials_StockAvailability_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_UnitOfMeasures AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (product_catalog_materials_UnitOfMeasures AS L_0 LEFT JOIN product_catalog_materials_UnitOfMeasures_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_DimensionUnits AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (product_catalog_materials_DimensionUnits AS L_0 LEFT JOIN product_catalog_materials_DimensionUnits_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_Currencies AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description
FROM (product_catalog_materials_Currencies AS L_0 LEFT JOIN product_catalog_materials_Currencies_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_sales_Suppliers AS SELECT
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  L_0.ID,
  coalesce(localized_1.Name, L_0.Name) AS Name,
  L_0.Address_Street,
  L_0.Address_City,
  L_0.Address_State,
  L_0.Address_PostalCode,
  L_0.Address_Country,
  L_0.Email,
  L_0.Phone,
  L_0.Fax
FROM (product_catalog_sales_Suppliers AS L_0 LEFT JOIN product_catalog_sales_Suppliers_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_sales_Months AS SELECT
  L_0.ID,
  coalesce(localized_1.Description, L_0.Description) AS Description,
  coalesce(localized_1.ShortDescription, L_0.ShortDescription) AS ShortDescription
FROM (product_catalog_sales_Months AS L_0 LEFT JOIN product_catalog_sales_Months_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = 'en');

CREATE VIEW localized_product_catalog_materials_ProductReview AS SELECT
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.ID,
  L.Name,
  L.Rating,
  L.Comment,
  L.Product_ID
FROM product_catalog_materials_ProductReview AS L;

CREATE VIEW localized_product_catalog_sales_OrderItems AS SELECT
  L.ID,
  L.Order_ID,
  L.Product_ID,
  L.Quantity
FROM product_catalog_sales_OrderItems AS L;

CREATE VIEW localized_product_catalog_sales_SalesData AS SELECT
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.ID,
  L.DeliveryDate,
  L.Revenue,
  L.Product_ID,
  L.Currency_ID,
  L.DeliveryMonth_ID
FROM product_catalog_sales_SalesData AS L;

CREATE VIEW localized_product_catalog_sales_Orders AS SELECT
  L.ID,
  L.Date,
  L.Customer
FROM product_catalog_sales_Orders AS L;

CREATE VIEW product_catalog_reports_Products AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID,
  ToAverageRating_1.AverageRating AS Rating,
  CASE WHEN Products_0.Quantity >= 8 THEN 3 WHEN Products_0.Quantity > 0 THEN 2 ELSE 1 END AS StockAvailability
FROM (product_catalog_materials_Products AS Products_0 LEFT JOIN product_catalog_reports_AverageRating AS ToAverageRating_1 ON ToAverageRating_1.ProductId = Products_0.ID);

CREATE VIEW localized_CatProd_Products AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Category_1.Name AS Category,
  Products_0.UnitOfMeasure_ID AS ToUnitOfMeasure_ID,
  Products_0.Currency_ID AS ToCurrency_ID,
  Products_0.Category_ID AS FoCategory_ID,
  Products_0.DimensionUnits_ID AS ToDimesionUnit_ID
FROM (localized_product_catalog_materials_Products AS Products_0 LEFT JOIN localized_product_catalog_materials_Categories AS Category_1 ON Products_0.Category_ID = Category_1.ID);

CREATE VIEW localized_CatProd_Supplier AS SELECT
  Suppliers_0.ID,
  Suppliers_0.Name,
  Suppliers_0.Address_Street,
  Suppliers_0.Address_City,
  Suppliers_0.Address_State,
  Suppliers_0.Address_PostalCode,
  Suppliers_0.Address_Country,
  Suppliers_0.Email,
  Suppliers_0.Phone,
  Suppliers_0.Fax
FROM localized_product_catalog_sales_Suppliers AS Suppliers_0;

CREATE VIEW localized_CatProd_StockAvailability AS SELECT
  StockAvailability_0.ID,
  StockAvailability_0.Description,
  StockAvailability_0.Product_ID AS ToProduct_ID
FROM localized_product_catalog_materials_StockAvailability AS StockAvailability_0;

CREATE VIEW localized_CatProd_VH_Categories AS SELECT
  Categories_0.ID AS Code,
  Categories_0.Name AS Text
FROM localized_product_catalog_materials_Categories AS Categories_0;

CREATE VIEW localized_CatProd_VH_UnitOfMeasure AS SELECT
  UnitOfMeasures_0.ID AS Code,
  UnitOfMeasures_0.Description AS Text
FROM localized_product_catalog_materials_UnitOfMeasures AS UnitOfMeasures_0;

CREATE VIEW localized_MyService_SuppliersProduct AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.CreationDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.Currency_ID,
  Products_0.DimensionUnits_ID,
  Products_0.Category_ID,
  Supplier_1.Address_Street,
  Supplier_1.Address_City,
  Supplier_1.Address_State,
  Supplier_1.Address_PostalCode,
  Supplier_1.Address_Country
FROM (localized_product_catalog_materials_Products AS Products_0 LEFT JOIN localized_product_catalog_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID)
WHERE (Supplier_1.Address_PostalCode = 98074) AND (Products_0.Name = 'Bread');

CREATE VIEW localized_MyService_SupplierToSales AS SELECT
  Supplier_1.Email,
  Category_2.Name,
  Currency_4.ID,
  Currency_4.Description
FROM ((((localized_product_catalog_materials_Products AS Products_0 LEFT JOIN localized_product_catalog_sales_Suppliers AS Supplier_1 ON Products_0.Supplier_ID = Supplier_1.ID) LEFT JOIN localized_product_catalog_materials_Categories AS Category_2 ON Products_0.Category_ID = Category_2.ID) LEFT JOIN localized_product_catalog_sales_SalesData AS SalesData_3 ON (SalesData_3.Product_ID = Products_0.ID)) LEFT JOIN localized_product_catalog_materials_Currencies AS Currency_4 ON SalesData_3.Currency_ID = Currency_4.ID);

CREATE VIEW localized_product_catalog_materials_SelProducts AS SELECT
  Products_0.createdAt,
  Products_0.createdBy,
  Products_0.modifiedAt,
  Products_0.modifiedBy,
  Products_0.ID,
